algoritmo "Maior que 10"
var
   v1, v2, result:inteiro
inicio
      escreva("Digite o primeiro valor: ")
      leia(v1)
      escreva("Digite o segundo valor: ")
      leia(v2)
      
      result <- v1+v2
      
      se result > 10 entao
      escreva(v1)
      senao
      escreva(v2)
      fimse
      
fimalgoritmo